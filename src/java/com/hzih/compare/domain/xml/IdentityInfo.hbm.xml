<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="identity_info" name="com.hzih.compare.domain.IdentityInfo">
    <id access="field" name="idCard" type="string"  length="50">
    </id>
    <property name="status"  access="field" type="int"/>
    <property name="code" length="50" access="field" type="string"/>
    <property name="xq" length="255" access="field" type="string"/>
    <property name="compare_time" length="50" access="field" type="date"/>
  </class>
</hibernate-mapping>
